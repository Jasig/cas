{
  "extends": [
    "config:base",
    ":preserveSemverRanges",
    ":rebaseStalePrs",
    "schedule:daily"
  ],
  "gradle": {
    "enabled": true
  },
  "ignoreDeps": [
    "com.github.coova:jradius",
    "com.mebigfatguy.fb-contrib:fb-contrib"
  ],
  "packageRules": [
    {
      "groupName": "Groovy",
      "packagePatterns": [
        "^org.codehaus.groovy:"
      ]
    },
    {
      "groupName": "JAXB2",
      "packagePatterns": [
        "^org.jvnet.jaxb2_commons:",
        "^com.sun.xml.bind"
      ]
    },
    {
      "groupName": "AmazonSDK",
      "packagePatterns": [
        "^com.amazonaws:"
      ]
    },
    {
      "groupName": "Swagger",
      "packagePatterns": [
        "^io.swagger:"
      ]
    }
  ]
}
